// Input no. of stairs --> Output no. of ways to climb.
// Each step is 1 or 2.

function stairs(N) {
    if (N === 0 || N === 1) {
        return 1;
    } else {
        const climb1 = stairs(N - 1);
        const climb2 = stairs(N - 2);
        return climb1 + climb2;
    }
}

const ans = stairs(10);
console.log(ans);
